datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model User {
  userId   String @id @db.Uuid @default(uuid())
  email    String @unique
  username String @unique
  name     String
  picture  String

  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt

  @@map("users")
}